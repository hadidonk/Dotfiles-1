^fg(\#169F85) ^i($HOME/.xmonad/.icons/net_up_01.xbm)^fg()  ${upspeedf enp2s0}  \
^fg(\#169F85) ^i($HOME/.xmonad/.icons/net_down_01.xbm)^fg()  ${downspeedf enp2s0}  \
^fg(\#169F85) ^i($HOME/.xmonad/.icons/vol2.xbm)^fg()  ${exec amixer | awk 'NR==5{print $4}' | awk '{gsub(/[[:punct:]]/,"")}1'  | gdbar -h 7 -w 60 -fg  '#FFFBF8' -bg  '#169F85' -ss 1 -sw 3  -nonl -s o -max 100 }  \
