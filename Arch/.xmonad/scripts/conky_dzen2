^fg(\#169F85) ^i($HOME/.xmonad/.icons/diskette.xbm)^fg()  ${exec  df -h / | sed -ne 's/^.* \([0-9]*\)% .*/\1/p'  | gdbar -h 7 -w 60 -fg  '#FFFBF8' -bg  '#169F85' -ss 1 -sw 3  -nonl -s o -max 101 }  \
^fg(\#169F85) ^i($HOME/.xmonad/.icons/cpu.xbm)^fg()  ${exec top -bn 2 | grep '^%Cpu' | tail -n 1 | gawk '{print $2+$4+$6}' | gdbar -h 7 -w 60 -fg  '#FFFBF8' -bg  '#169F85' -ss 1 -sw 3  -nonl -s o -max}  \
^fg(\#169F85) ^i($HOME/.xmonad/.icons/arch1.xbm)^fg()  ${exec pacaur -k | awk 'END {print NR}' }  \
